<!DOCTYPE html>
<html lang="en">

<head>
    <title>Pali Meals</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <meta name="keywords" content="Meals, Pali" />
    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <!--// Meta tag Keywords -->

    <!-- Custom-Files -->
    <link rel="stylesheet" href="css/bootstrap.css">
    <!-- Bootstrap-Core-CSS -->
    <link rel="stylesheet" href="css/style.css" type="text/css" media="all" />
    <!-- Style-CSS -->

    <!-- Font-Awesome-Icons-CSS -->
    <!-- //Custom-Files -->

    <!-- Web-Fonts -->
    <link
        href="//fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i&amp;subset=latin-ext"
        rel="stylesheet">
    <link
        href="//fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&amp;subset=latin-ext"
        rel="stylesheet">
    <!-- //Web-Fonts -->

</head>

<body>
    <!-- header -->
    <header>
        <nav class="navbar navbar-expand-lg navbar-light py-4">
            <div class="container">
                <h1>
                    <a class="navbar-brand" href="index.ejs">
                        <i class="fas fa-utensils"></i>Pali Meals API
                    </a>
                </h1>
                <button class="navbar-toggler ml-md-auto" type="button" data-toggle="collapse"
                    data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div style="display:none" class="loading"><img src="images/loading.gif" height="30" width="30" alt="">
                </div>
            </div>
        </nav>
    </header>

    <!-- //header -->

    <!-- banner -->
    <div class="callbacks_container">
        <ul class="rslides" id="slider3">
            <li>
                <div class="slider-info bg2">
                    <div class="w3l-overlay">
                        <div class="banner-text text-center container">
                            <h3 class="text-white mb-md-4 mb-3">Want to prepare a
                                <span>Tasty Meal?</span>
                            </h3>
                            <p class="movetxt text-white mb-4">Checkout how much ingredients it will cost</p>
                            <div class="search-agile">
                                <form action="#" method="post">
                                    <input type="search" class="meal_ids"
                                        placeholder="Search with IDs here...Eg: 52776,52779,52780" required="">
                                    <button type="submit" class="search">Sort by Ingredients</button>
                                    <div class="clearfix"> </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
        </ul>
    </div>
    <!-- //banner -->

    <!-- middle section two -->
    <div style="display:none" class="agile-wthree-works py-5 mt-5" id="some">
        <h3 class="title text-center text-dark mb-lg-5"><span class="meal_name"></span> MealID:  <span class="meal_id"></span></h3>
        <div class="container py-xl-5 py-lg-3">
            <div class="row">
                <div class="grids-w3ls-right-2">
                            
                            
                </div>
                <div class="grids-w3ls-right-2 offset-lg-7 offset-sm-4">
                    <h4 class="title text-dark mb-sm-5 mb-4">Category:  <span  class="category"></span></h4>
                    <p class="mt-4"><b>How to Prepare:  </b><span  class="prepare"></span></p>
                    <a href="#" class="button-w3ls mt-5" >View on Youtube</a>
                </div>
            </div>
        </div>
    </div>
    <!-- //middle section two -->

    <!-- footer -->
    <footer>
        <!-- copyright -->
        <div class="copy_right">
            <p class="text-center text-white py-sm-4 py-3">Â© 2018 Pali Meals. All rights reserved | Design by
                <a href="#">Vitalis</a>
            </p>

        </div>
        <!-- //copyright -->
    </footer>
    <!-- //footer -->


    <!-- Js files -->
    <!-- JavaScript -->
    <script src="js/jquery-2.2.3.min.js"></script>
    <!-- Default-JavaScript-File -->

    <!-- banner slider -->
    <script src="js/responsiveslides.min.js"></script>
    <script>
        // You can also use "$(window).load(function() {"
        $(function () {
            // Slideshow 4
            $("#slider3").responsiveSlides({
                auto: true,
                pager: false,
                nav: false,
                speed: 800,
                namespace: "callbacks",
                before: function () {
                    $('.events').append("<li>before event fired.</li>");
                },
                after: function () {
                    $('.events').append("<li>after event fired.</li>");
                }
            });

        });
    </script>
    <!-- //banner slider -->

    <script src="js/bootstrap.js"></script>
    <!-- Necessary-JavaScript-File-For-Bootstrap -->

    <script src="js/script.js"></script>
    <!-- Script to take care of the request -->
    <!-- //Js files -->

</body>

</html>